{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "deploy": "npm run build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "run-p dev:*",
    "postinstall": "remix setup cloudflare && patch-package",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js --watch --debug",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@jsquash/avif": "1.0.1",
    "@jsquash/jpeg": "^1.1.5",
    "@jsquash/png": "^2.0.0",
    "@jsquash/resize": "^1.0.1",
    "@jsquash/webp": "^1.1.3",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@remix-run/cloudflare-workers": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/server-runtime": "^1.3.5",
    "cross-env": "^7.0.3",
    "get-rgba-palette": "^2.0.1",
    "is-svg": "^4.3.2",
    "js-image-lib": "^0.1.7",
    "mime-tree": "^0.1.4",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-image": "^0.3.16",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@cloudflare/wrangler": "^1.19.11",
    "@remix-run/dev": "^1.3.5",
    "@remix-run/eslint-config": "^1.3.5",
    "@types/lru-cache": "^7.6.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "esbuild": "0.14.34",
    "eslint": "^8.13.0",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "@cloudflare/kv-asset-handler": "^0.2.0"
  },
  "browser": {
    "./server.js": "./server.browser.js"
  }
